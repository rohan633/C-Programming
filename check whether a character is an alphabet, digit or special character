
#include <stdio.h>

int main() {
    
    char ch;
    printf(" Enter the character ");
    scanf ("%c",&ch);
    
    if ((ch>='a'&&ch<='z')||(ch>='A'&&ch<='Z'))
    {
     printf(" Enter Character is Alphabet");   
    }
    else if (ch>='0'&& ch<='9')
    {
        printf(" Enter character is Digits");
    }
    else
    {
        printf("Enter character is Special Character");
    }
    return 0;
}
