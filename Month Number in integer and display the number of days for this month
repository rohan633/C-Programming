#include<stdio.h>
void main()
{
    int month;
    
    printf(" Enter the Month Number");
    scanf ("%d",&month);
    
    switch (month)
    {
        case 1:
        printf("Month have 31 day");
        break;
        
        case 2:
        printf(" Month have 29 day");
        printf(" If month is leap year then 28 day");
        break;
        
        case 3:
        printf(" Month have 31 day");
        break;
        
        case 4:
        printf(" Month have 30 day");
        break;
        
        case 5:
        printf(" Month have 31 day");
        break;
        
        case 6:
        printf(" Month have 30 day");
        break;
        
        case 7:
        printf(" Month have 31 day");
        break;
        
        case 8:
        printf(" Month have 31 day");
        break;
        
        case 9:
        printf(" Month have 30 day");
        break;
        
        case 10:
        printf(" Month have 31 day");
        break;
        
        case 11:
        printf(" Month have 30 day");
        break;
        
        case 12:
        printf(" Month have 31 day");
        break;
    }
   
    
}
