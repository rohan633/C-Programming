#include <stdio.h>

int main() {
    
    int m;
    printf("Enter the m value");
    scanf("%d",&m);
    
   if(m == 0)
   {
       printf(" The value of n = 0");
   }
   if (m>0)
   {
       printf(" The value of n = 1");
   }
   if (m<0)
   {
       printf(" The value of n = -1");
       }
   
    return 0;
}
