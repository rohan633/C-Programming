#include<stdio.h>

int main()
{
    int a;
    printf(" Enter your Height in centimeter :");
    scanf("%d",&a);
    
   if (a < 150.0)
        printf("The person is Dwarf. \n");
    else if ((a >= 150.0) && (a < 165.0))
        printf("The person is  average heighted. \n");
    else if ((a  >= 165.0) && (a  <= 195.0))
        printf("The person is taller. \n");
    else
        printf("Abnormal height.\n");
    
}
